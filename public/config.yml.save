backend:
  name: git-gateway
  branch: master
collections:
  - name: "content"
    label: "Blog"
    folder: "content"
    create: true
    slug: "index"
    fields:
      - {label: "Title", name: "title", widget: "string"}
      - {label: 'Sub Title', name: 'metatitle', widget: 'string' }
      - {label: "Publish Date", name: "date", widget: "datetime"}
      - {label: "Body", name: "body", widget: "markdown"}
